#include <stdio.h>
int main(){
    for (int i=0;i<5;i++){
        for (int j=0;j>=2*(i-5)-1;j--){
            printf(" ");
        }
        for (int k=0;k<=i;k++){
        printf(" *");
    }
    printf("  |");
     for (int j=0;j>=2*(i-5)-1;j--){
            printf(" ");
        }
        for (int k=0;k<=i;k++){
        printf(" %d",k+1);
    }
    printf("  |");
      for (int j=0;j>=2*(i-5)-1;j--){
            printf(" ");
        }
        for (int k=0;k<=i;k++){
        printf(" %c",'A'+k);
    }

        printf("\n");
    }

    return 0;
}
