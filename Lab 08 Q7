#include <stdio.h>
int main(){
      for (int i=-1;i<8;i++){
        i++;
        for (int j=8;j>=i;j--){
            printf(" ");
        }
        for (int k=0;k<=i;k++){
        printf(" *");
    }
     for (int j=i;j<=8;j++){
        printf(" ");
        }
        printf("|");
    
     for (int j=8;j>=i;j--){
            printf(" ");
        }
        for (int k=0;k<=i;k++){
        printf(" %d",k+1);
    }
      for (int j=i;j<=8;j++){
        printf(" ");
        }
        printf("|");
        for (int j=8;j>=i;j--){
        printf(" ");
        }
        for (int k=0;k<=i;k++){
        printf(" %c",'A'+k);
    }
        printf("\n");
    }

    return 0;
}
